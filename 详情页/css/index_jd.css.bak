/*全局样式*/
body,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,p{
	padding:0px;
	margin:0px;
}
a{
	color:#666;
	text-decoration:none;
}
a:hover{
	color:#e4393c;
	text-decoration:underline;
}
img{
	vertical-align:bottom;
}
ul,ol{
	list-style:none;
}
body{
	font-size:12px;
	color:#333;
}
input{
	border:none;
}
.lf{
	float:left;
}
.rt{
	float:right;
}
.clear{
	clear:both;
}
.red{
	color:red;
}
p.p_header{
	background:#F7F7F7;
	line-height:25px;
	padding-left:10px;
	border-bottom:1px solid #ddd;
	font-size:14px;
	
}
.ul_header{
	background:#f7f7f7;
	border:1px solid #ddd;
	border-top:2px solid #ccc;
	border-left:none;
	height:35px;
}
.ul_header ul li{
	float:left;
	padding:0px 10px;
	font-size:14px;
	line-height:35px;
}
.ul_header ul li.focus{
	background:#fff;
	height:39px;
	border:1px solid #ddd;
	border-top:2px solid #e4393c;
	border-bottom:none;
	margin-top:-5px;
	
}
.ul_header ul li.focus a{
	color:red;
}
.clear_float{
	float:none;
}
#top_main,#nav,#banner,#main,#footer_box,#footer,#adv{
	width:1211px;
	margin:0 auto;
	
}
/*header样式*/
#top_box{
	background:#f7f7f7;
